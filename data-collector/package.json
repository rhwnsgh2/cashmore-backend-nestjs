    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^22.10.7",
    "esbuild": "^0.24.0",
    "typescript": "^5.7.3",
    "vitest": "^4.0.17"
  }
}
