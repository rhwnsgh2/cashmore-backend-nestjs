{
  "name": "cashmore-data-collector",
  "version": "0.0.1",
  "description": "AppLovin data collector for BigQuery",
  "private": true,
  "scripts": {
    "build": "esbuild src/handlers/applovin.ts --bundle --platform=node --target=node20 --outfile=dist/applovin.js --external:@google-cloud/bigquery",
